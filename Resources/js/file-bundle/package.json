{
  "name": "file-bundle-js",
  "version": "1.0.0",
  "description": "Add a compact, modern and full-featured file browser",
  "main": "client/index.js",
  "scripts": {
    "jasmine": "node_modules/.bin/jasmine JASMINE_CONFIG_PATH=./jasmine.json || echo",
    "test-jasmine": "npm run start | npm run jasmine",
    "test": "./node_modules/mocha/bin/mocha --compilers js:babel-register 'test/tests/reducers' --recursive || echo",
    "test-watch": "./node_modules/mocha/bin/mocha --compilers js:babel-register 'test/tests/reducers' --recursive --watch || echo",
    "test-suite": "PORT=5050 ./node_modules/mocha/bin/mocha --compilers js:babel-register 'test/tests' --recursive || echo",
    "test-suite2": "./node_modules/mocha/bin/mocha --compilers js:babel-register 'test/tests' --recursive || echo",
    "test-api": "PORT=5050 ./node_modules/mocha/bin/mocha --compilers js:babel-register 'test/tests/api/index.js' || echo",
    "start": "babel-node test/server/src/index.js",
    "start-test": "npm start | npm run test-suite",
    "start-test2": "npm start | npm run test-suite2",
    "watch-css": "./node_modules/.bin/node-sass -w ../../scss -o ./test/server/assets/css --include-path ./node_modules",
    "watch-js": "watchify index.js -t [ babelify ] -o test/server/assets/js/app.js --debug --verbose",
    "watch": "npm run watch-css | npm run watch-js",
    "build-css": "node-sass ../../scss/app.scss -o ./test/server/assets/css --include-path ./node_modules",
    "build-js": "browserify index.js -t [ babelify ] -o test/server/assets/js/app.js",
    "build": "npm run build-css | npm run build-js",
    "build-min": "NODE_ENV=production browserify index.js -t [ babelify ] --debug | exorcist test/server/assets/js/app.min.js.map | uglifyjs -c > test/server/assets/js/app.min.js",
    "watch-phantom": "watchify --exclude webpage --exclude system --exclude fs ./test/tests/phantom/src/index.js -t [ babelify ] -o ./test/tests/phantom/tests.compiled.es5 --debug --verbose"
  },
  "author": "tweede golf",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "7.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "^6.14.0",
    "babelify": "7.3.0",
    "browserify": "13.1.0",
    "chai": "3.5.0",
    "eslint": "3.7.1",
    "eslint-plugin-babel": "3.3.0",
    "eslint-plugin-react": "6.4.0",
    "exorcist": "0.4.0",
    "jasmine-es6": "0.4.0",
    "less": "2.7.1",
    "mocha": "3.0.2",
    "node-sass": "3.10.1",
    "uglify": "0.1.5",
    "watchify": "3.7.0"
  },
  "dependencies": {
    "babel-cli": "6.16.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.9.0",
    "body-parser": "1.15.2",
    "bootstrap-sass": "3.3.7",
    "connect-busboy": "0.0.2",
    "express": "4.14.0",
    "filesize": "3.3.0",
    "font-awesome": "4.6.3",
    "gm": "1.23.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-file-drag-and-drop": "^0.1.6",
    "react-redux": "4.4.5",
    "redux": "3.5.2",
    "redux-logger": "2.6.1",
    "superagent": "^2.0.0",
    "util": "0.10.3"
  }
}
