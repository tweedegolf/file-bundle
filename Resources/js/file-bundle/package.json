{
  "name": "file-bundle",
  "version": "1.0.0",
  "description": "Add a compact, modern and full-featured file browser to your Symfony 3 projects",
  "main": "client/index.js",
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha --compilers js:babel-register 'js/test/tests' --recursive || echo",
    "test-watch": "./node_modules/mocha/bin/mocha --compilers js:babel-register 'js/test/tests' --recursive --watch || echo",
    "test-api": "./node_modules/mocha/bin/mocha --compilers js:babel-register 'js/test/tests/api.js' --recursive || echo",
    "start": "babel-node js/test/server/src/index.js",
    "watch": "watchify js/index.js -t [ babelify ] -o js/test/server/build/assets/js/app.js --debug --verbose",
    "build": "NODE_ENV=production browserify js/index.js -t [ babelify ] --debug | exorcist js/test/server/build/assets/js/app.min.js.map | uglifyjs -c > js/test/server/build/assets/js/app.min.js"
  },
  "author": "tweede golf",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "7.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "^6.14.0",
    "babelify": "7.3.0",
    "browserify": "13.1.0",
    "chai": "3.5.0",
    "eslint": "3.7.1",
    "eslint-plugin-babel": "3.3.0",
    "eslint-plugin-react": "6.4.0",
    "exorcist": "0.4.0",
    "less": "2.7.1",
    "mocha": "3.0.2",
    "uglify": "0.1.5",
    "watchify": "3.7.0"
  },
  "dependencies": {
    "babel-cli": "6.16.0",
    "babel-polyfill": "^6.9.0",
    "body-parser": "1.15.2",
    "connect-busboy": "0.0.2",
    "express": "4.14.0",
    "filesize": "3.3.0",
    "gm": "1.23.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-file-drag-and-drop": "^0.1.6",
    "react-redux": "4.4.5",
    "redux": "3.5.2",
    "redux-logger": "2.6.1",
    "superagent": "^2.0.0",
    "util": "0.10.3"
  }
}
