{
  "name": "file-bundle-js",
  "version": "1.0.0",
  "description": "Add a compact, modern and full-featured file browser",
  "main": "client/index.js",
  "scripts": {
    "eslint": "./node_modules/.bin/eslint",
    "phantom": "./node_modules/phantomjs-prebuilt/lib/phantom/bin/phantomjs ./Resources/js/test/tests/phantom/tests.compiled.es5",
    "jasmine": "babel-node ./Resources/js/test/tests/index.js",
    "test": "npm start | npm run jasmine",
    "start": "babel-node ./Resources/js/test/server/src/index.js",
    "watch-server": "nodemon --exec babel-node ./Resources/js/test/server/src/index.js",
    "watch-css": "./node_modules/.bin/node-sass -w ./Resources/scss -o ./Resources/js/test/server/assets/css --include-path ./node_modules",
    "watch-js": "watchify ./Resources/js/src/index.js -t [ babelify ] --verbose --debug -o ./Resources/js/test/server/assets/js/app.js",
    "watch": "npm run watch-css | npm run watch-js",
    "build-css": "node-sass ./Resources/scss/app.scss -o ./Resources/js/test/server/assets/css --include-path ./node_modules",
    "build-js": "browserify ./Resources/js/src/index.js -t [ babelify ] -o ./Resources/js/test/server/assets/js/app.js",
    "build": "npm run build-css | npm run build-js",
    "build-min": "NODE_ENV=production browserify ./Resources/js/index.js -t [ babelify ] --debug | exorcist ./Resources/js/test/server/assets/js/app.min.js.map | uglifyjs -c > ./Resources/js/test/server/assets/js/app.min.js",
    "build-phantom": "browserify --exclude webpage --exclude system --exclude fs ./Resources/js/test/tests/phantom/src/index.js -t [ babelify ] -o ./Resources/js/test/tests/phantom/tests.compiled.es5",
    "watch-phantom": "watchify --exclude webpage --exclude system --exclude fs ./Resources/js/test/tests/phantom/src/index.js -t [ babelify ] -o ./Resources/js/test/tests/phantom/tests.compiled.es5 --debug --verbose",
    "gulp:develop": "NODE_ENV=develop ./node_modules/.bin/gulp develop"
  },
  "author": "tweede golf",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "7.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "^6.14.0",
    "babelify": "7.3.0",
    "browserify": "13.1.0",
    "eslint": "3.17.1",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-babel": "4.1.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.0",
    "exorcist": "0.4.0",
    "flow-bin": "0.42.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-autoprefixer": "3.1.1",
    "gulp-concat": "2.6.1",
    "gulp-livereload": "3.8.1",
    "gulp-sass": "3.1.0",
    "gulp-sourcemaps": "2.4.1",
    "gulp-util": "3.0.8",
    "jasmine": "2.5.2",
    "less": "2.7.1",
    "mapstraction": "1.0.1",
    "node-sass": "3.10.1",
    "phantomjs-prebuilt": "2.1.14",
    "uglify": "0.1.5",
    "vinyl-buffer": "1.0.0",
    "vinyl-source-stream": "1.1.0",
    "watchify": "3.7.0"
  },
  "dependencies": {
    "babel-cli": "6.24.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.23.0",
    "body-parser": "1.15.2",
    "bootstrap-sass": "3.3.7",
    "classnames": "2.2.5",
    "connect-busboy": "0.0.2",
    "eslint": "3.17.1",
    "eslint-config-airbnb": "14.1.0",
    "express": "4.14.0",
    "filesize": "3.3.0",
    "font-awesome": "4.6.3",
    "gm": "1.23.0",
    "ramda": "0.23.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-file-drag-and-drop": "^0.1.6",
    "react-redux": "5.0.3",
    "redux": "3.6.0",
    "redux-logger": "2.8.2",
    "redux-persist": "4.5.0",
    "superagent": "^2.0.0",
    "util": "0.10.3"
  }
}
