{
    "name": "file-bundle-js",
    "version": "1.0.0",
    "description": "Add a compact, modern and full-featured file browser",
    "main": "./src/js/index.js",
    "scripts": {
        "phantom": "./node_modules/phantomjs-prebuilt/lib/phantom/bin/phantomjs ./tests/phantom/tests.compiled.es5",
        "jasmine": "./node_modules/.bin/babel-node ./tests/index.js",
        "test": "npm run build-phantom && npm start | npm run jasmine",
        "test2": "npm run build-phantom && npm run jasmine",
        "start": "API=1 PORT=8000 ./node_modules/.bin/babel-node ./server/src/index.js",
        "watch-server": "nodemon --exec babel-node ./server/src/index.js",
        "build-phantom": "browserify --exclude webpage --exclude system --exclude fs ./tests/phantom/src/index.js -t [ babelify ] -o ./tests/phantom/tests.compiled.es5",
        "watch-phantom": "watchify --exclude webpage --exclude system --exclude fs ./tests/phantom/src/index.js -t [ babelify ] -o ./tests/phantom/tests.compiled.es5 --debug --verbose",
        "gulp:develop": "./node_modules/.bin/gulp develop",
        "gulp:production": "./node_modules/.bin/gulp production",
        "webpack": "webpack --display-error-details",
        "webpack:production": "webpack -p --display-error-details",
        "develop_with_local_server": "PORT=8000 npm run gulp:develop",
        "develop": "npm run gulp:develop",
        "build": "npm run gulp:production && npm run webpack:production && gulp copy",
        "copy": "cpx '../backend/src/TweedeGolf/FileBundle/**' ../build",
        "dist": "npm run build && npm run copy"
    },
    "author": "tweede golf",
    "license": "MIT",
    "devDependencies": {
        "babel-eslint": "^8.0.1",
        "babel-loader": "^7.1.2",
        "babel-plugin-transform-flow-strip-types": "6.22.0",
        "babel-preset-es2015": "^6.9.0",
        "babel-preset-flow": "6.23.0",
        "babel-preset-react": "6.11.1",
        "babel-preset-stage-0": "6.5.0",
        "babel-register": "^6.14.0",
        "babelify": "7.3.0",
        "browserify": "13.1.0",
        "core-js": "^2.4.1",
        "cpx": "^1.5.0",
        "eslint": "^4.9.0",
        "eslint-config-airbnb": "^16.1.0",
        "eslint-plugin-babel": "^4.1.2",
        "eslint-plugin-flowtype": "^2.39.1",
        "eslint-plugin-import": "^2.8.0",
        "eslint-plugin-jsx": "^0.0.2",
        "eslint-plugin-jsx-a11y": "^6.0.2",
        "eslint-plugin-react": "^7.4.0",
        "exorcist": "0.4.0",
        "flow-bin": "^0.52.0",
        "gulp": "github:gulpjs/gulp#4.0",
        "gulp-autoprefixer": "3.1.1",
        "gulp-concat": "2.6.1",
        "gulp-copy": "^1.0.1",
        "gulp-livereload": "3.8.1",
        "gulp-sass": "3.1.0",
        "gulp-sourcemaps": "2.4.1",
        "gulp-util": "3.0.8",
        "jasmine": "2.6.0",
        "less": "2.7.1",
        "node-sass": "3.10.1",
        "phantomjs-prebuilt": "2.1.14",
        "uglify": "0.1.5",
        "vinyl-buffer": "1.0.0",
        "vinyl-source-stream": "1.1.0",
        "watchify": "3.7.0",
        "webpack": "^3.3.0"
    },
    "dependencies": {
        "babel-cli": "6.24.0",
        "babel-core": "^6.26.0",
        "babel-preset-env": "^1.6.1",
        "body-parser": "1.15.2",
        "bootstrap-sass": "3.3.7",
        "classnames": "2.2.5",
        "connect-busboy": "0.0.2",
        "core-js": "^2.4.1",
        "envify": "^4.1.0",
        "eslint-plugin-flow": "^2.29.1",
        "express": "4.14.0",
        "filesize": "3.3.0",
        "font-awesome": "4.6.3",
        "gm": "1.23.0",
        "http-proxy-middleware": "^0.17.4",
        "i18next": "^8.2.1",
        "i18next-browser-languagedetector": "^1.0.1",
        "prop-types": "^15.6.0",
        "ramda": "0.24",
        "react": "^16.0.0",
        "react-dom": "^16.0.0",
        "react-file-drag-and-drop": "^0.1.6",
        "react-i18next": "^6.0.6",
        "react-redux": "^5.0.6",
        "react-redux-subspace": "^2.0.6",
        "redux": "^3.7.2",
        "redux-logger": "2.8.2",
        "redux-persist": "^5.0.1",
        "redux-subspace": "^2.0.6",
        "redux-thunk": "^2.2.0",
        "reselect": "^3.0.1",
        "superagent": "^2.0.0",
        "util": "0.10.3",
        "yamljs": "^0.2.10"
    }
}
